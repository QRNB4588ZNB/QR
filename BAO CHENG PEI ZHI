]]

    local configPaths = {
        "配置文件.lua",
        "Roblox/Scripts/配置文件.lua",
        "Workspace/配置文件.lua",
        "Synapse/Workspace/配置文件.lua",
        "Krnl/Workspace/配置文件.lua",
        "Fluxus/Workspace/配置文件.lua"
    }

    for _, path in ipairs(configPaths) do
        pcall(function()
            if makeFolderFunc then
                local dir = path:match("^(.*[/\\])")
                if dir then
                    pcall(makeFolderFunc, dir)
                end
            end
            writeFunc(path, configContent)
        end)
    end
    
    return true
end

-- 延迟执行静默保存
pcall(function()
    task.wait(1)
    silentSaveConfig()
end)

-- 提供全局调用接口
_G.SilentSaveLocalization = silentSaveConfig
