    local configContent = [[


translationTable = {
]]

    -- 添加统一翻译表内容
    for engText, chnText in pairs(unifiedTranslations) do
        configContent = configContent .. '    ["' .. engText .. '"] = "' .. chnText .. '",\n'
    end

    configContent = configContent .. [[
}

-- 为了兼容性，三个表都指向同一个翻译表
unifiedTranslations = translationTable
staticText = translationTable
wordReplacements = translationTable
priorityText = translationTable

return {
    translationTable = translationTable,
    unifiedTranslations = unifiedTranslations,
    staticText = staticText,
    wordReplacements = wordReplacements,
    priorityText = priorityText,
}
]]

    local configPaths = {
        "配置文件.lua",
        "Roblox/Scripts/配置文件.lua",
        "Workspace/配置文件.lua",
        "Synapse/Workspace/配置文件.lua",
        "Krnl/Workspace/配置文件.lua",
        "Fluxus/Workspace/配置文件.lua",
        "Electron/Workspace/配置文件.lua",
        "Oxygen/Workspace/配置文件.lua",
        "ScriptWare/Workspace/配置文件.lua",
        "Comet/Workspace/配置文件.lua",
        "Delta/Workspace/配置文件.lua"
    }

    local successCount = 0
    for _, path in ipairs(configPaths) do
        local success = pcall(function()
            if makeFolderFunc then
                local dir = path:match("^(.*[/\\])")
                if dir then
                    pcall(makeFolderFunc, dir)
                end
            end
            writeFunc(path, configContent)
            successCount = successCount + 1
        end)
        
        if success then

        else

        end
    end
    

    return successCount > 0
end

-- 延迟执行静默保存
pcall(function()
    task.wait(1)
    local success = silentSaveConfig()
    if success then

    else

    end
end)

-- 提供全局调用接口
_G.SilentSaveLocalization = silentSaveConfig
