-- 静默汉化配置文件保存脚本（全平台全注入器适配）
-- 特点：无任何提示，后台自动保存，全平台全注入器适配
-----------------------------------------------------------
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer or Players.PlayerAdded:Wait()

-- 全注入器文件操作函数适配
local writeFunc, getPathFunc, makeFolderFunc

-- Synapse X / 忍者注入器
if type(syn) == "table" then
    writeFunc = syn.writefile or writefile
    getPathFunc = syn.datapath or function() return "Synapse/Workspace" end
    makeFolderFunc = syn.makefolder or makefolder

-- Krnl注入器
elseif type(krnl) == "table" then
    writeFunc = writefile
    getPathFunc = function() return "Krnl/Workspace" end
    makeFolderFunc = makefolder

-- Fluxus注入器
elseif type(fluxus) == "table" then
    writeFunc = writefile
    getPathFunc = function() return "Fluxus/Workspace" end
    makeFolderFunc = makefolder

-- Electron注入器
elseif type(electron) == "table" then
    writeFunc = writefile
    getPathFunc = function() return "Electron/Workspace" end
    makeFolderFunc = makefolder

-- Oxygen U注入器
elseif type(oxygen) == "table" then
    writeFunc = writefile
    getPathFunc = function() return "Oxygen/Workspace" end
    makeFolderFunc = makefolder

-- Script-Ware注入器
elseif type(scriptware) == "table" then
    writeFunc = writefile
    getPathFunc = function() return "ScriptWare/Workspace" end
    makeFolderFunc = makefolder

-- Comet注入器
elseif type(comet) == "table" then
    writeFunc = writefile
    getPathFunc = function() return "Comet/Workspace" end
    makeFolderFunc = makefolder

-- Delta注入器
elseif type(getexecutorname) == "function" and string.find(getexecutorname():lower(), "delta") then
    writeFunc = writefile
    getPathFunc = function() return "Delta/Workspace" end
    makeFolderFunc = makefolder

-- 通用注入器适配
else
    writeFunc = writefile or writeFile
    getPathFunc = function() 
        return "Roblox/Scripts"
    end
    makeFolderFunc = makefolder
end

-- 静默保存配置
